pipeline {
    agent any
    stages {
        stage('Setup') {
            steps {
                sh 'pip install robot-framework'
                sh 'pip install robotframework-databaselibrary'
                sh 'pip install pymssql'
            }
        }
        stage('Tests: Robot Framework') {
            steps {
                sh 'robot .'
            }
        }
    }
}
